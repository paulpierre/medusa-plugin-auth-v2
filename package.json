{
  "name": "medusa-plugin-auth-v2",
  "version": "1.0.0",
  "description": "Authentication plugin for Medusa V2",
  "keywords": [
    "medusa-plugin",
    "medusa-v2",
    "authentication",
    "oauth",
    "google",
    "facebook",
    "github"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
