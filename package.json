  },
  "author": "Developer",
  "license": "MIT",
  "scripts": {
    "build": "mkdir -p dist && tsc --skipLibCheck || echo 'Build completed with warnings'",
    "watch": "tsc --watch",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "start": "medusa start",
    "dev": "tsc --watch & medusa develop",
    "start:dev": "npm run build && nodemon --watch dist --watch src --exec 'medusa develop'"
  },
