    "@medusajs/utils": "^1.9.3",
    "awilix": "^8.0.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.0",
    "medusa-core-utils": "^1.2.0",
